{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  },
  "ts-node": {
    "esm": true,
    "transpileOnly": true
  }
}
